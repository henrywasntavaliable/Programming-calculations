
import math
def distance_between_points():
    x_1 = float(input("Please enter X1"))
    x_2 = float(input("Please enter X2"))
    y_1 = float(input("Please enter Y1"))
    y_2 = float(input("Please enter Y2"))

    distance = math.sqrt((x_2 - x_1)**2 + (y_2 - y_1)**2)
    print(distance)

distance_between_points()
